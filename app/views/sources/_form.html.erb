<%= form_for @source do |f| %>

<%= render 'shared/error_messages', object: f.object %> 

<div class="row">

<div class="col-md-4">
	<div class="form-group">
		<%= f.label '信息来源' %>
		<%= f.text_field :content, class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.submit %>
	</div>

</div>
</div>
<% end %>
